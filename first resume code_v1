<script>
    var coll = document.getElementsByClassName("collapsible");
    var i;

    for (i = 0; i < coll.length; i++) {
        coll[i].addEventListener("click", function() {
            this.classList.toggle("active");
            var content = this.nextElementSibling;
            if (content.style.display === "block") {
                content.style.display = "none";
                this.innerHTML = this.innerHTML.replace("fa-minus", "fa-plus");
            } else {
                content.style.display = "block";
                this.innerHTML = this.innerHTML.replace("fa-plus", "fa-minus");
            }
        });
    }

    var skills = document.querySelectorAll('.skill');
    skills.forEach(function(skill) {
        var skillBarFill = skill.querySelector('.skill-bar-fill');
        skillBarFill.style.width = skill.getAttribute('data-skill');
        
        skill.addEventListener('mouseover', function() {
            skillBarFill.style.width = '0';
            setTimeout(function() {
                skillBarFill.style.width = skill.getAttribute('data-skill');
            }, 0);
        });
    });
</script>

